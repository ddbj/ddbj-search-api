    "/api/entries/bioproject/{identifier}": {
      "get": {
        "summary": "BioProject detail",
        "tags": [
          "search result detail"
        ],
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "required": true,
            "name": "identifier",
            "in": "path"
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "identifier": {
                      "type": "string"
                    },
                    "dateCreated": {
                      "type": [
                        "string",
                        "null"
                      ]
                    },
                    "dateModified": {
                      "type": [
                        "string",
                        "null"
                      ]
                    },
                    "datePublished": {
                      "type": [
                        "string",
                        "null"
                      ]
                    },
                    "type": {
                      "type": "string",
                      "enum": [
                        "bioproject"
                      ]
                    },
                    "title": {
                      "type": "string"
                    },
                    "organism": {
