{
  "openapi": "3.1.0",
  "info": {
    "title": "DDBJ SEARCH API",
    "version": "0.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:5137",
      "description": "Development server"
    }
  ],
  "components": {
    "schemas": {},
    "parameters": {}
  },
  "paths": {
    "/api/entries/": {
      "get": {
        "summary": "All entries list",
        "tags": [
          "search result list"
        ],
        "parameters": [
          {
            "schema": {
              "type": "string",
              "description": "integer string; treat as '1' if omitted."
            },
            "required": false,
            "description": "integer string; treat as '1' if omitted.",
            "name": "page",
            "in": "query"
          },
          {
            "schema": {
              "type": "string",
              "description": "integer string; treat as '10' if omitted."
            },
            "required": false,
            "description": "integer string; treat as '10' if omitted.",
            "name": "perPage",
            "in": "query"
          },
          {
            "schema": {
              "type": "string",
              "description": "Comma separated keywords",
              "example": "homo sapience,Draparnaldia"
            },
            "required": false,
            "description": "Comma separated keywords",
            "name": "keywords",
            "in": "query"
          },
          {
            "schema": {
              "type": "string",
              "example": "2020-05-01,2021-04-30"
            },
            "required": false,
            "name": "datePublished",
            "in": "query"
          },
          {
            "schema": {
              "type": "string",
              "example": "2020-05-01,2021-04-30"
            },
            "required": false,
            "name": "dateUpdated",
            "in": "query"
          },
          {
            "schema": {
              "type": "string",
              "description": "Comma separated list of DB types",
              "example": "biosample,bioproject"
            },
            "required": false,
            "description": "Comma separated list of DB types",
            "name": "types",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "pagination": {
                      "type": "object",
                      "properties": {
                        "page": {
                          "type": "integer",
                          "minimum": 1,
                          "example": 1
                        },
                        "perPage": {
                          "type": "integer",
                          "minimum": 1,
                          "maximum": 100,
                          "example": 10
                        },
                        "total": {
                          "type": "integer",
                          "minimum": 0,
                          "example": 10000
                        }
                      },
                      "required": [
                        "page",
                        "perPage",
                        "total"
                      ]
                    },
                    "items": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "identifier": {
                            "type": "string"
                          },
                          "type": {
                            "type": "string",
                            "example": "bioproject"
                          },
                          "title": {
                            "type": "string",
                            "example": "Draparnaldia sp. CCAC 6921, genomic data."
                          },
                          "dbXrefs": {
                            "type": "object",
                            "additionalProperties": {
                              "type": "number"
                            },
                            "example": {
                              "bioproject": 1,
                              "biosample": 1,
                              "sra-study": 2
                            }
                          },
                          "datePublished": {
                            "type": "string",
                            "example": "2013-05-31"
                          }
                        },
                        "required": [
                          "identifier",
                          "type",
                          "title",
                          "dbXrefs",
                          "datePublished"
                        ]
                      }
                    }
                  },
                  "required": [
                    "pagination",
                    "items"
                  ]
                }
              }
            }
          }
        }
      }
    },
